{
  "name": "@pacexy/eslint-config",
  "type": "module",
  "version": "0.9.0",
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
  "description": "ESLint config based on @antfu/eslint-config",
  "author": "pacexy <pacexy@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/pacexy/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pacexy/eslint-config.git"
  },
  "bugs": "https://github.com/pacexy/eslint-config/issues",
  "keywords": [
    "eslint-config"
  ],
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "pnpm run build:typegen",
    "build:inspector": "eslint-config-inspector build",
    "build:typegen": "node scripts/typegen.js",
    "dev": "eslint-config-inspector --open false",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "postversion": "git push --follow-tags && npm publish",
    "prepare": "simple-git-hooks",
    "update": "corepack use pnpm@latest && pnpm update --latest",
    "version": "pnpm run build"
  },
  "dependencies": {
    "@antfu/eslint-config": "catalog:prod",
    "eslint-plugin-format": "catalog:prod",
    "local-pkg": "catalog:prod"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "catalog:peer.react",
    "@eslint/config-inspector": "catalog:dev",
    "@types/node": "catalog:dev",
    "@types/react": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-plugin-react-google-translate": "catalog:peer.react",
    "eslint-plugin-react-hooks": "catalog:peer.react",
    "eslint-plugin-react-refresh": "catalog:peer.react",
    "eslint-typegen": "catalog:dev",
    "lint-staged": "catalog:dev",
    "simple-git-hooks": "catalog:dev"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
